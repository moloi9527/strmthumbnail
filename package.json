{
  "name": "emby-thumbnail-manager",
  "version": "2.0.0",
  "description": "Emby 封面自动生成器 - 增强版",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "docker:build": "docker build -t emby-thumbnail-manager .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "bcrypt": "^5.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "validator": "^13.11.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "lru-cache": "^10.1.0",
    "sharp": "^0.33.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
